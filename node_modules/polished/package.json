{
  "name": "polished",
  "description": "A lightweight toolset for writing styles in Javascript.",
  "license": "MIT",
  "author": "Brian Hough <hello@brianhough.net> (https://polished.js.org)",
  "homepage": "https://polished.js.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/styled-components/polished.git"
  },
  "bugs": {
    "url": "https://github.com/styled-components/polished/issues"
  },
  "version": "3.5.1",
  "main": "lib/index.js",
  "module": "dist/polished.es.js",
  "scripts": {
    "build": "yarn build:lib && yarn build:dist && yarn build:flow && yarn build:docs && yarn build:typescript",
    "prebuild:dist": "shx rm -rf dist/*",
    "build:dist": "rollup -c",
    "build:docs": "yarn build:docs:site",
    "prebuild:docs:site": "shx rm -rf docs/*",
    "build:docs:site": "documentation build src/** -t docs-theme --github -o docs -f html -c ./.documentation.json",
    "postbuild:docs:site": "shx cp CNAME docs/CNAME && shx cp dist/polished.js docs/assets/",
    "build:flow": "flow-copy-source -v -i '{**/test/*.js,**/*.test.js}' src lib",
    "prebuild:lib": "shx rm -rf lib/*",
    "build:lib": "cross-env BABEL_ENV=cjs babel --out-dir lib src --ignore test.js",
    "build:typescript": "tsgen \"lib/**/*.js.flow\" --ignore \"lib/**/_*.js.flow\"",
    "prebuild:umd": "shx rm -rf dist/*",
    "build:watch": "npm-watch",
    "docs": "pushstate-server docs",
    "flow": "flow check && flow batch-coverage src/ --show-all --strip-root",
    "lint": "eslint src",
    "prepare": "yarn build && yarn typescript",
    "semantic-release": "semantic-release",
    "test": "jest src",
    "typescript": "tsc ./typescript-test.ts --noEmit --target es6 --module es2015 --moduleResolution node --allowJs"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "sideEffects": false,
  "types": "lib/index.d.ts",
  "dependencies": {
    "@babel/runtime": "^7.8.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-flow": "^7.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-preval": "4.0.0",
    "concat-stream": "^2.0.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "documentation": "12.1.4",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "flow-bin": "^0.120.1",
    "flow-copy-source": "^2.0.8",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "lodash": "^4.17.15",
    "npm-watch": "^0.6.0",
    "prettier": "^1.18.2",
    "pushstate-server": "3.1.0",
    "ramda": "^0.27.0",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-uglify": "^6.0.3",
    "semantic-release": "^17.0.4",
    "shx": "^0.3.2",
    "tsgen": "1.3.0",
    "typescript": "3.8.3",
    "validate-commit-msg": "^2.14.0",
    "vinyl": "^2.2.0",
    "vinyl-fs": "^3.0.2"
  },
  "keywords": [
    "color",
    "color manipulate",
    "color manipulation",
    "colour",
    "css-in-js",
    "curried color manipulation",
    "emotion",
    "flow",
    "glamor",
    "inline-styles",
    "polished",
    "react",
    "styled-components",
    "typescript"
  ],
  "_args": [
    [
      "polished@3.5.1",
      "C:\\Portfolio\\Portfolio"
    ]
  ],
  "_from": "polished@3.5.1",
  "_id": "polished@3.5.1",
  "_inBundle": false,
  "_integrity": "sha512-GVbvskpBiDV5TknurGL6OyFfLHsCknxbU8w5iMppT8rW0tLEoQHrIRfrPNPqGXNj3HGhkjRvhmg59Fy7HSnCAw==",
  "_location": "/polished",
  "_phantomChildren": {},
  "_requested": {
    "type": "version",
    "registry": true,
    "raw": "polished@3.5.1",
    "name": "polished",
    "escapedName": "polished",
    "rawSpec": "3.5.1",
    "saveSpec": null,
    "fetchSpec": "3.5.1"
  },
  "_requiredBy": [
    "/baseui"
  ],
  "_resolved": "https://registry.npmjs.org/polished/-/polished-3.5.1.tgz",
  "_spec": "3.5.1",
  "_where": "C:\\Portfolio\\Portfolio",
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/polished"
  },
  "husky": {
    "hooks": {
      "post-commit": "validate-commit-msg",
      "pre-commit": "lint-staged --debug"
    }
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testURL": "http://localhost/",
    "verbose": true
  },
  "lint-staged": {
    "src/**/!(*.test).js": [
      "prettier --no-semi --single-quote --trailing-comma all --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.test.js": [
      "prettier --no-semi --single-quote --trailing-comma all --write",
      "eslint --fix",
      "git add"
    ]
  },
  "watch": {
    "build:docs": "src/**/*.js",
    "build:lib": "src/**/*.js"
  }
}
