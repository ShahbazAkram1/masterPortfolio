  "description": "ESlint loader for webpack",
  "license": "MIT",
  "author": "Maxime Thirouin",
  "homepage": "https://github.com/webpack-contrib/eslint-loader",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webpack-contrib/eslint-loader.git"
